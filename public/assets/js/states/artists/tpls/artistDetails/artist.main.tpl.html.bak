<div class="artistChannelDiv">
<figure class="ti pr">
	<div id="burnsbox">
		<img ng-if="artistDetails.artistcover" ng-src="{{artistDetails.artistcover}}" style="border-top:none" alt="{{name}}">
		<img ng-if="!artistDetails.artistcover" ng-src="/demobilleder/hlowq.jpg" />				
	</div>
<!-- 				<div class="contests">
				<div id="campaigncomp"></div>
				<img id="camlogo2" src="https://static.musik.dk/upload/files/konkurrence.svgz" alt="MusikDK Konkurrence">
				<figcaption class="metacamp">
					<h2></h2>
				</figcaption>
				<a href="/konkurrencer/{{contest_id}}">
					<div class="gotocomp"></div>
				</a>
			</div> -->

			<div class="mhlogo"></div>
			<figcaption class="artistmeta">
				<h1>{{artistDetails.artistname}}</h1>
			</figcaption>
		</figure>
		<div class="pr">
			<!-- HØJRE KOLONNE -->
			<section id="rightSection" class="fr w350">
				<div ng-if="artistDetails.campaignLinks.length">
				<!-- STREAMING SERVICES-->
					<header class="h">
						<span class="fe i-play hicon" aria-hidden="true"></span>
						<h2>Afspil på</h2>				
					</header>
					<div class="ip2">
						<a ng-repeat="stream in artistDetails.campaignLinks" href="{{stream.link}}" target="_blank">
							<button ng-if="stream.service === 'Spotify'" class="btnspotify iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'ITunes'" class="btnitunes iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'Deezer'" class="btndeezer iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'WiMP'" class="btnwimp iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'TDC Play'" class="btntdcplay iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'YouMusic'" class="btnyoumusic iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'Rdio'" class="btnrdio iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'Telmore Music'" class="btntelmore iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'Google Music'" class="btngooglemusic iebgfix">&nbsp;</button>
							<button ng-if="stream.service === 'Tidal'" class="btntidal iebgfix">&nbsp;</button>
						</a>
					</div>					
				<!-- STREAMING SERVICES END -->
				</div>
			<div ng-if="artistDetails.biography">
				<!-- BIOGRAPHY -->
				<header class="h">
					<span class="fe i-book hicon" aria-hidden="true"></span>
					<h2>Biografi</h2>
				</header>
				<section class="ip">
					<div class="artistBio">
						<p>{{artistDetails.biography}}</p>
					</div>
					<br>
					<p class="infotext floatright">Tekst fra Wikipedia</p>
				</section>
				<!-- BIOGRAHY END -->
			</div>
			<div ng-if="artistDetails.nextconcerts">
				<!-- CONCERTS -->
				<header class="h" ng-switch on="nextShows">
					<span class="fe i-ticket hicon" aria-hidden="true"></span>
					<h2 ng-switch-when="1">Næste koncert</h2>
					<h2 ng-switch-when="2">Næste 2 koncerter</h2>
					<h2 ng-switch-when="3">Næste 3 koncerter</h2>
				</header>
				<div class="ip">			
					<ul class="ll">
						<li class="carrow" ng-repeat="live in artistDetails.nextconcerts">
							<article>
								<div ng-if="live.delivery_status == 'Cancelled'" class="ribbonWrapRed">
									<div class="ribbonRed">AFLYST</div>
								</div>

								<div ng-if="live.delivery_status == 'Sold Out'" class="ribbonWrapRed">
									<div class="ribbonRed">UDSOLGT</div>
								</div>
								<a href="{{live.deeplink}}" target="_blank">
									<div class="cimage-small">
										<img ng-src="{{artistDetails.artistcover}}" class="lazyload" width="80" height="80" alt="{{artistDetails.name}} på MusikDK">
										<div class="ctime" datetime="{{live.date | amDateFormat:'dd.MM.YY'}}">{{live.date | amDateFormat:'MMM'}}<span>{{live.date | amDateFormat:'DD'}}</span></div>
									</div>
									<div class="clinfo-small">	 
										<h3 class="clloc ellipsis">{{live.venue_name}}</h3>
										<h4>{{live.venue_city}}</h4>
										<h5>{{live.date | amDateFormat: 'hh:mm'}}</h5>
									</div>							
								</a>
							</article>
						</li>
					</ul>
					<a ui-sref="artist.concerts">
						<button class="btnblue" type="button">Find alle koncertdatoer</button>
					</a>
				</div>
				<!-- CONCERTS END -->				
			</div>		
			<div ng-if="artistDetails.links">
				<!-- SOCIAL MEDIA LINKS -->
				<header class="h">
					<span class="fe i-export hicon" aria-hidden="true"></span>
					<h2>Links</h2>
				</header>
				<section class="ip4">
					<div id="socialLinksWrap">
						<div id="socialLinks" style="width:100%">
							<ul style="list-style-type:none;">
								<li ng-if="artistDetails.facebook" class="fl" style="width:25%;max-width:85px;">
									<a href="{{artistDetails.facebook}}" target="_blank"><i class="fblink roundedcorners"></i></a>								
								</li>
								<li ng-if="artistDetails.twitter" class="fl" style="width:25%;max-width:85px;">
									<a href="{{artistDetails.twitter}}" target="_blank"><i class="twlink roundedcorners"></i></a>
								</li>
								<li ng-if="artistDetails.instagram" class="fl" style="width:25%;max-width:85px;">
									<a href="{{artistDetails.instagram}}" target="_blank"><i class="iglink roundedcorners"></i></a>
								</li>
								<li ng-if="artistDetails.souncloud" class="fl" style="width:25%;max-width:85px;">
									<a href="{{artistDetails.souncloud}}" target="_blank"><i class="sclink roundedcorners"></i></a>
								</li>
							</ul>						
						</div>
					</div>
				</section>			
				<!-- SOCIAL MEDIA LINKS END -->					
			</div>
		</section>
		<section id="leftSection" class="mcl mr350">
			<div id="rssDiv" class="pr" ng-if="artistDetails.rss.length">
				<!-- RSS NEWS -->
				<header class="h rlt">
					<span class="fe i-open-book hicon" aria-hidden="true"></span>
					<h2>Nyheder</h2>
				</header>
				<div class="tabs lt">
					<div class="ipno c news pc50">
						<article ng-repeat="rss in artistDetails.rss | limitTo:artistDetails.limits.rss.showing">
							<a href="{{rss.link}}" target="_blank">
								<img class="lazyload" ng-src="{{rss.source.logo}}" width="100" alt="{{rss.title}}">
								<h4>{{rss.source.name}}</h4>
								<h2>{{rss.title}}</h2>
								<time class="newstime" am-time-ago="rss.pubDate" am-preprocess="unix"></time>
								<span class="font-entypo icon-export cicon pa" aria-hidden="true"></span>
							</a>
						</article>
						<button class="btnwhite btnmore btnrss" type="button" ng-click="showMoreItemsOnList('rss')">Vis flere nyheder</button>
					</div>			
				</div>
				<!-- RSS NEWS END-->
			</div>
			<div class="pr" ng-if="artistDetails.videos.length">
				<!-- VIDEOS -->
				<header class="h">
					<span class="fe i-video hicon" aria-hidden="true"></span>
					<h2>Videoer</h2>
				</header>
				<div class="tabs lt">
					<div class="#1 artistVideos">
						<div class="ip2 c video pc3to2">
							<article ng-repeat="videos in artistDetails.videos">
								<a class="vidPopup" data-ytid="{{videos.ytid}}">
									<img class="lazyload" ng-src="https://i1.ytimg.com/vi/{{videos.ytid}}/hqdefault.jpg" alt="{{videos.title}}">
									<div class="vidol">
										<span>AFSPIL</span>
									</div>
									<div class="ellipsis">
										<h3 class="vn">{{videos.title}}</h3> <br>
										<h3 class="van"></h3>
									</div>
									<span class="fe i-play cicon" aria-hidden="true"></span>
								</a>
							</article>	
						</div>
					</div>			
				</div>
				<!-- VIDEOS END -->					
			</div>
			<div id="socialMediaDiv" ng-if="artistDetails.socialmedia.length">
				<!-- SOCIAL UPDATES	 -->
				<header class="h">
					<span class="fe i-layout hicon" aria-hidden="true"></span>
					<h2>Sociale opdateringer</h2>
				</header>
				<div id="supmain" class="socialMedia">
					<ul id="suptiles">
						<li ng-repeat="feed in artistDetails.socialmedia | limitTo:artistDetails.limits.socialmedia.showing">
							<a href="{{feed.url}}" target="_blank">
								<img ng-src="{{feed.image}}" width="300" />
								<section class="ip">		
									<h2></h2>
									<p>{{feed.content}}</p>
									<h3>
										<time class="timeago">
											<span am-time-ago="feed.pubDate" am-preprocess="unix"></span>
										</time> |	{{feed.type}}
									</h3>
								</section>
							</a>
						</li>							
					</ul>
				</div>			
				<!-- SOCIAL UPDATES	END -->					
			</div>
		</section>
	</div>	
</div>